class Solution {
public:
    bool isPalindrome(int x) {
        int t=x;
        if(t<0)
           return false;
        vector<int> vec;
        while(t/10){
            int i=t%10;
            vec.push_back(i);
            t/=10;
        }
        vec.push_back(t);
        int y=0;
        int j=0;
        while(vec.size()!=0){
            y=y+vec.back()*pow(10,j);
            vec.pop_back();
            ++j;
        }
        if(y==x)
           return true;
        return false;
    }
};

class Solution {
public:
    bool isPalindrome(int x) {
        int t=x;
        if(t<0)
           return false;
        int result=0;
        while(t/10){
           result=result*10+t%10;
            t/=10;
        }
        result=result*10+t;
        
        if(result==x)
           return true;
        return false;
    }
};
